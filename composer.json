{
  "name": "silverstripe/installer",
  "description": "The SilverStripe Framework Installer",
  "require": {
    "php": ">=5.3.2",
    "silverstripe/cms": "3.2.1",
    "silverstripe/framework": "3.2.1",
    "silverstripe/siteconfig": "*",
    "gdmedia/ss-auto-git-ignore": "dev-master",
    "unclecheese/zen-fields" : "*",
    "unclecheese/betterbuttons": "1.2.*",
    "unclecheese/dropzone": "dev-master",
    "undefinedoffset/sortablegridfield": "dev-master",
    "sheadawson/silverstripe-linkable": "~1.2@dev",
    "camspiers/autoloader-composer-silverstripe": "1.0.*",
    "jonom/focuspoint": "dev-master"
  },
  "config": {
    "process-timeout": 600
  },
  "extra": {
    "installer-paths": {
      "public_html/{$name}": ["type:silverstripe-module"]
    }
  },
  "scripts": {
     "post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
  },
  "gdmedia/ss-auto-git-ignore": "dev-master",
  "minimum-stability": "dev"
}
